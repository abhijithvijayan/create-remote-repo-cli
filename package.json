		"url": "https://abhijithvijayan.in"
	},
	"bin": "./app/index.js",
	"files": [
		"/app",
		"demo.gif"
	],
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"npm run lint:fix",
			"git add"
		]
	},
